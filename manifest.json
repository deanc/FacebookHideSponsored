{
    "content_scripts": [ {
        "all_frames": true,
        "css": ["hideit.css"],
        "js": [ "mutation_summary.js", "jquery.js", "jquery.mutation-summary.js", "hideit.js" ],
        "matches": [ "https://*.facebook.com/*", "http://*.facebook.com/*" ],
        "run_at": "document_start"
    } ],
    "description": "Hides sponsored stories from your Facebook news feed",
    "manifest_version": 2,
    "name": "Hide Facebook sponsored stories",
    "version": "2.1"
}
